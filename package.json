{
  "name": "@ecainternational/eca-components",
  "description": "ECA Component Library",
  "version": "1.0.3",
  "type": "module",
  "private": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ECAInternational/eca-components.git"
  },
  "publishConfig": {
    "@ECAInternational:registry": "https://npm.pkg.github.com"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "watch": "rollup --watch -c --bundleConfigAsCjs",
    "clean": "rimraf ./dist",
    "test": "test-storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storybook-docs": "storybook build  --docs",
    "chromatic": "chromatic --exit-zero-on-changes",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@ecainternational/eca-design-tokens": "^1.0.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@storybook/addon-actions": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/cli": "^7.5.3",
    "@storybook/jest": "^0.2.3",
    "@storybook/manager-api": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/test-runner": "^0.14.1",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.5.3",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.16",
    "boxen": "^5.0.1",
    "chromatic": "^9.0.0",
    "clean-publish": "^4.2.0",
    "daisyui": "^4.0.4",
    "dedent": "^0.7.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "polished": "^4.2.2",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.25.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "storybook": "^7.5.3",
    "storybook-addon-data-theme-switcher": "^0.1.7",
    "tailwindcss": "^3.3.5",
    "vite": "^4.1.4",
    "zx": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
